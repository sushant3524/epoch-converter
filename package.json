{
	"name": "epoch-converter",
	"displayName": "Epoch-Converter",
	"description": "Converts timestamps into Date",
	"version": "0.0.1",
	"publisher": "makhan",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:epoch-converter.convert",
		"onCommand:epoch-converter.reverse"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "epoch-converter.convert",
				"title": "Convert"
			},
			{
				"command": "epoch-converter.reverse",
				"title": "Reverse"
			}
		],
        "keybindings": [
            {
                "command": "epoch-converter.convert",
                "key": "shift+alt+c",
                "mac": "shift+alt+c"
            },
			{
                "command": "epoch-converter.reverse",
                "key": "shift+alt+r",
                "mac": "shift+alt+r"
            }
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sushant3524/epoch-converter.git"
	  }
}
